@page
@using Microsoft.EntityFrameworkCore
@inject House2Invest.Data.ApplicationDbContext _contexto
@model OfertaModel
@{
    ViewData["Title"] = "Oferta";
    var _appconfig = new House2Invest.Models.AppPerfil();
    var _usulogado = new UsuarioApp();
    try
    {
        _appconfig =
            _contexto.AppPerfil
            .Include(v => v.AppConfiguracoes)
            .Include(x => x.AppConfiguracoes.AppConfiguracoes_Aplicativo)
            .Include(y => y.AppConfiguracoes.AppConfiguracoes_Azure)
            .FirstOrDefault();

        _usulogado =
            _contexto.Users
            .FirstOrDefault(x => x.UserName == User.Identity.Name);
    }
    catch (Exception) { }
}
<style>
    .fundpress-inner-welcome-section .fundpress-inner-welcome-content {
        position: relative;
        padding: 300px 0 30px;
        z-index: 101;
    }

    .countdown_box {
        position: initial;
        right: initial;
        bottom: initial;
        width: 100%;
        z-index: 5;
        background-image: none;
        display: block;
        margin: auto auto;
    }

        .countdown_box .countdown-section {
            display: inline-block;
            text-align: center;
            position: relative;
            padding: 4px 0;
            margin: 0 11px;
            line-height: 30px;
            font-size: 12px;
            white-space: nowrap;
            min-width: 30px;
        }

        .countdown_box .countdown-amount {
            line-height: 20px;
            font-weight: bold;
            font-size: 30px;
            padding-bottom: 2px;
            display: block;
        }

    .imagem-slider-oferta {
        background-repeat: no-repeat;
        background-size: cover;
        min-height: 400px;
        max-height: 800px;
    }

    .kt-portlet .kt-portlet__head .kt-portlet__head-label .kt-portlet__head-title {
        margin: 0;
        padding: 0;
        font-size: 1.8rem;
        font-weight: 500;
        color: #464457;
    }

    .fundpress-grid-item-content {
        background-color: #FFFFFF;
        -webkit-box-shadow: none;
        box-shadow: none;
    }
</style>
