                    if (investiu) {
                        $.ajax({
                            //ype: "POST",
                            url: "/api/sistema/delreg?objeto=" + tabela + "&codigo=" + id,
                            beforeSend: function (xhr) {
                                xhr.setRequestHeader("XSRF-TOKEN", $('input:hidden[name="__RequestVerificationToken"]').val());
                            },
                            complete: function () { },
                            contentType: false,
                            processData: false,
                            success: function (response) {
                                if (response.length == 0)
                                    swal("Opsss", "Erros impediram uma ação de cancelamento de investimento", "error");
                                else {
                                    if (response.mens == "ok") {
                                        swal("Sucesso", "Cancelamento do investimento efetuado com sucesso", "success")
                                            .then((value) => {
                                                window.location.reload();
                                            });
                                    } else {
                                        swal("Erro", response.mens, "error");
                                    }
                                }
                            },
                            error: function (e) { }
                        });
                    }
                    else { }
                });
        }

